<script>
    document.addEventListener('DOMContentLoaded', function() {
        const textElement = document.querySelector(".text-animation");
        const aboutBoxInner = document.querySelector(".about-box-back");
        let text = textElement.innerHTML;

        textElement.innerHTML = text.split('').map(char => {
            if (char === ' ') {
                return '<span class="space"> </span>';
            }
            return `<span>${char}</span>`;
        }).join('');

        const spans = textElement.querySelectorAll(".text-animation span");
        spans.forEach((span, index) => {
            const randomX = (Math.random() * 2 - 1) * window.innerWidth * 1 + 'px';
            const randomY = (Math.random() * 2 - 1) * window.innerHeight * 1 + 'px';
            const randomRotation = Math.random() * 720 + 'deg';

            span.style.setProperty('--random-x', randomX);
            span.style.setProperty('--random-y', randomY);
            span.style.setProperty('--random-rotation', randomRotation);

            span.style.animationDelay = `${1 * 0.5}s`;
        });
    });
    </script>

